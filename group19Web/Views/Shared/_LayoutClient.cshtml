    <script type="text/javascript">
        function removeCartItemMenu(id, cart) {
            
        jQuery
          .ajax({
            method: "POST",

            url: "/ajax/removecart",

            data: { id: id },
          })

          .done(function (msg) {
            if (cart == "cart") {
              location.reload();
            } else {
              jQuery("#box-cart").html(msg);

              var totalCart = jQuery("#total-item").val();

              jQuery("#num-cart-price").text(0);

              jQuery("#num-cart").text(totalCart);

              jQuery("#dropdownMenu2").trigger("click");
            }
          });
      }

        function addCart(productId) {
            
          var qty = parseInt(jQuery("#qty" + productId).val());
            var price = parseFloat(jQuery("#getPrice").val());
            var name = jQuery("#productName").val()
            console.log(qty + " " + price + " " + productId + " " + name);
            var cartItem = {};
            cartItem.quantity = qty;
            cartItem.productId = productId;
            cartItem.priceUnit = price;
            cartItem.productName = name;
            cartItem.finalTotal = price * qty;
        if (qty <= 0) {
          jQuery("#qty" + productId).val(1);

          return false;
        } else {
          jQuery
            .ajax({
              method: "POST",

              url: "/Client/addCart",

                data: '{cartItem: ' + JSON.stringify(cartItem) + '}',
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                success: function () {
                    // alert("Data has been added successfully.");  
                    alert("Thêm hàng thành công");
                },
                error: function () {
                    alert("Lỗi thêm hàng");
                }
            })

